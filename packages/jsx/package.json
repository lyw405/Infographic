{
  "name": "@antv/infographic-jsx",
  "version": "0.0.1-beta.3",
  "description": "JSX runtime for AntV Infographic",
  "keywords": [
    "antv",
    "infographic",
    "jsx"
  ],
  "license": "MIT",
  "author": "Aarebecca",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./lib/index.js",
      "types": "./esm/index.d.ts"
    },
    "./jsx-runtime": {
      "import": "./esm/jsx-runtime.js",
      "require": "./lib/jsx-runtime.js",
      "types": "./esm/jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./esm/jsx-runtime.js",
      "require": "./lib/jsx-runtime.js",
      "types": "./esm/jsx-runtime.d.ts"
    }
  },
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "files": [
    "lib",
    "esm"
  ],
  "scripts": {
    "build": "run-s clean build:esm build:cjs",
    "build:cjs": "MODULE=cjs vite build",
    "build:esm": "MODULE=esm vite build && tsc --emitDeclarationOnly --outDir esm",
    "build:test": "tsc --noEmit -p __tests__",
    "ci": "run-s test build lint",
    "clean": "rm -rf lib esm tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts,.tsx && npm run build:test",
    "test": "vitest --run --coverage",
    "watch": "MODULE=esm vite build --watch"
  },
  "devDependencies": {
    "@types/react": "^19.1.12"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  }
}
