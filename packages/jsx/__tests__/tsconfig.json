{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": null,
    "noEmit": true,
    "composite": false,
    "moduleResolution": "node",
    "paths": {
      "@antv/infographic-jsx": ["../src/index"],
      "@antv/infographic-jsx/*": ["../src/*"],
      "@@/*": ["./*"]
    }
  },
  "include": ["./**/*", "../src/global.d.ts"],
  "exclude": ["node_modules"],
  "references": [{ "path": ".." }]
}
